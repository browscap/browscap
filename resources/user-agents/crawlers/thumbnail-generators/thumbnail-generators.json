{
  "division": "Thumbnail Generators",
  "sortIndex": 170,
  "lite": false,
  "standard": true,
  "userAgents": [
    {
      "userAgent": "Thumbnail Generators",
      "properties": {
        "Parent": "DefaultProperties",
        "Comment": "Thumbnail Generators",
        "Browser": "Thumbnail Generators",
        "Browser_Type": "Bot/Crawler",
        "Crawler": "true"
      },
      "children": [
        {
          "match": "Mozilla/5.0 (#PLATFORM#) Gecko* ThumbShotsBot*",
          "device": "Linux Desktop",
          "properties": {
            "Browser": "ThumbShotsBot"
          },
          "platforms": [
            "Linux_64", "Linux"
          ]
        },
        {
          "match": "thumbshots-de-bot*",
          "properties": {
            "Browser": "thumbshots-de-bot",
            "Browser_Maker": "thumbshots.de"
          }
        },
        {
          "match": "ThumbnailAgent",
          "properties": {
            "Browser": "ThumbnailAgent"
          }
        },
        {
          "match": "ThumbSniper *",
          "properties": {
            "Browser": "ThumbSniper",
            "Browser_Maker": "Thomas Schulte"
          }
        },
        {
          "match": "gosquared-thumbnailer/1.0*",
          "properties": {
            "Browser": "gosquared-thumbnailer",
            "Browser_Maker": "Go Squared Ltd.",
            "Version": "1.0"
          }
        },
        {
          "match": "gosquared-thumbnailer/*",
          "properties": {
            "Browser": "gosquared-thumbnailer",
            "Browser_Maker": "Go Squared Ltd."
          }
        },
        {
          "match": "Ruby, link_thumbnailer*",
          "properties": {
            "Browser": "link_thumbnailer",
            "Browser_Maker": "Pierre-Louis Gottfrois"
          }
        },
        {
          "match": "Thumbor/3.14*",
          "properties": {
            "Browser": "Thumbor",
            "Version": "3.14"
          }
        },
        {
          "match": "Thumbor/*",
          "properties": {
            "Browser": "Thumbor"
          }
        },
        {
          "match": "Mozilla/5.0 (compatible; FTT2 Toplist Thumb Grabber*",
          "properties": {
            "Browser": "FTT2 Toplist Thumb Grabber",
            "Browser_Maker": "Fast Traffic Trader 2"
          }
        },
        {
          "match": "Mozilla/5.0 (*) applewebkit* (*khtml*like*gecko*) khtml2png Safari/*",
          "properties": {
            "Browser": "khtml2png",
            "Browser_Maker": "Hauke Goos-Habermann"
          }
        },
        {
          "match": "Easy-Thumb*",
          "properties": {
            "Browser": "Easy-Thumb Bot",
            "Browser_Maker": "Easy-Thumb.net"
          }
        },
        {
          "match": "Mozilla/5.0 (*Linux*) AppleWebKit/* (KHTML, like Gecko) WordPress.com mShots Safari/*",
          "properties": {
            "Browser": "Wordpress.com MShots Thumbnailer",
            "Browser_Maker": "Automattic"
          }
        },
        {
          "match": "Mozilla/5.0 (*Macintosh*) AppleWebKit/* (*khtml*like*gecko*) Like-Version/* Safari/* webkit2png/*",
          "properties": {
            "Browser": "webkit2png",
            "Browser_Maker": "Paul Hammond"
          }
        },
        {
          "match": "Mozilla/5.0 (*Windows*) applewebkit* (*khtml*like*gecko*) Chrome/* Safari/* (*shrinktheweb.com*",
          "properties": {
            "Browser": "ShrinkTheWeb Bot",
            "Browser_Maker": "ShrinkTheWeb"
          }
        }
      ]
    }
  ]
}
