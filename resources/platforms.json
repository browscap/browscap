{
  "platforms": {
    "ChromeOS": {
      "match": "*CrOS*",
      "properties": {
        "Platform": "ChromeOS",
        "Platform_Description": "Google Chrome OS",
        "Win32": "false"
      }
    },
    "OSX": {
      "match": "*Intel Mac OS X*",
      "properties": {
        "Platform": "MacOSX",
        "Win32": "false"
      }
    },
