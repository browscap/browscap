{
  "name": "browscap/browscap",
  "description": "Browser Capabilities Tools",
  "license": "MIT",
  "type": "application",
  "keywords": [
    "browscap"
  ],
  "authors": [
    {
      "name": "Joshua Estes",
      "email": "f1gm3nt@gmail.com",
      "homepage": "http://joshuaestes.me"
    },
    {
      "name": "James Titcumb",
      "email": "james@asgrim.com",
      "homepage": "http://www.jamestitcumb.com"
    },
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/browscap/browscap/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/browscap/browscap",
  "support": {
    "issues": "https://github.com/browscap/browscap/issues",
    "source": "https://github.com/browscap/browscap"
  },
  "require": {
    "php": ">=8.1.0,<8.3.0",
    "ext-json": "*",
    "beberlei/assert": "^3.3.2",
    "ergebnis/json-normalizer": "^4.2.0",
    "mimmi20/ua-browser-type": "^9.0.2",
    "mimmi20/ua-device-type": "^9.0.2",
    "monolog/monolog": "^3.4.0",
    "seld/jsonlint": "^1.10.0",
    "symfony/console": "^v6.3.4",
    "symfony/finder": "^v6.3.3",
    "twig/twig": "^v3.7.1"
  },
  "require-dev": {
    "ext-zip": "*",
    "browscap/browscap-php": "^7.2.0",
    "doctrine/coding-standard": "^12.0.0",
    "league/flysystem": "^3.16.0",
    "matthiasmullie/scrapbook": "^1.5.1",
    "mikey179/vfsstream": "^v1.6.11",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.34",
    "phpstan/phpstan-beberlei-assert": "^1.1.2",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.14",
    "phpunit/phpunit": "^10.3.5"
  },
  "suggest": {
    "ext-zip": "to create a zip file containing all other generated files"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Browscap\\": "src/Browscap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapTest\\": "tests/BrowscapTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "bin": [
    "bin/browscap"
  ],
  "config": {
    "platform": {
      "php": "8.1.3"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
