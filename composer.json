  "require": {
    "ext-json": "*",
    "php": "^7.1",
    "beberlei/assert": "^2.9 || ^3.0",
    "localheinz/json-normalizer": "^0.5.2",
    "mimmi20/ua-browser-type": "^6.0.2",
    "mimmi20/ua-device-type": "^6.0.0",
    "monolog/monolog": "^1.23",
    "seld/jsonlint": "^1.6",
    "symfony/console": "^3.3 || ^4.0",
    "symfony/finder": "^3.3 || ^4.0",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "browscap/browscap-php": "^4.1",
    "friendsofphp/php-cs-fixer": "^2.12",
    "mikey179/vfsstream": "^1.6",
    "pepakriz/phpstan-exception-rules": "^0.7",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-beberlei-assert": "^0.11",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.2"
  },
  "suggest": {
    "ext-zip": "to create a zip file containing all other generated files"
  },
  "autoload": {
    "psr-4": {
      "Browscap\\": "src/Browscap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapTest\\": "tests/BrowscapTest/",
      "UserAgentsTest\\": "tests/UserAgentsTest/"
    }
  },
  "prefer-stable": true,
  "bin": [
    "bin/browscap"
  ],
  "support": {
    "issues": "https://github.com/browscap/browscap/issues",
    "source": "https://github.com/browscap/browscap"
  }
}
